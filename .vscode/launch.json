{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: debug server-side",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
			"cwd": "${workspaceFolder}${pathSeparator}app",
      "skipFiles": [
        "${workspaceFolder}${pathSeparator}app/node_modules/**/*.js",
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
